    "prod": "NODE_ENV=production node server.js"
  },
  "dependencies": {
    "@material-ui/core": "^4.5.0",
    "@material-ui/icons": "^4.4.3",
    "@material-ui/styles": "^4.5.0",
    "@svgr/webpack": "^4.3.3",
    "cheerio": "^1.0.0-rc.3",
    "clsx": "^1.0.4",
    "cross-fetch": "^3.0.4",
    "delegate": "^3.2.0",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.1.1",
    "next-offline": "^4.0.6",
    "node-mocks-http": "^1.8.0",
    "path-to-regexp": "^3.1.0",
    "qs": "^6.9.0",
    "react": "16.10.2",
    "react-dom": "^16.11.0",
    "react-image-magnify": "^2.7.4",
    "react-loading-skeleton": "^1.2.0",
    "react-swipeable-views": "^0.13.3",
    "react-visibility-sensor": "^5.1.1",
    "resize-observer-polyfill": "^1.5.1",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "devDependencies": {
    "babel-plugin-transform-imports": "^2.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^2.1.2",
    "jsdom": "^15.2.1",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "jest": {
    "clearMocks": true,
    "testEnvironment": "./test/config/jsdom",
    "setupFiles": [
      "./test/config/setup.js"
    ],
    "setupFilesAfterEnv": [
      "./test/config/mocks.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/react-storefront/**/*.{js,jsx}",
      "!**/index.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|svg)$": "../test/config/assetMock.js",
      "react-storefront/(.*)": "<rootDir>/src/react-storefront/$1"
    }
  }
